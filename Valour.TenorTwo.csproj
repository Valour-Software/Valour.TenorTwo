<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>TenorTwo - A lightweight C# TenorAPI V2 implementation</Title>
        <Description>A Tenor API implementation built for V2 of the API using no dependencies</Description>
        <PackageProjectUrl>https://github.com/Vooper-Media-LLC/Valour.TenorTwo</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/Vooper-Media-LLC/Valour.TenorTwo/blob/main/LICENSE</PackageLicenseUrl>
        <PackageIcon>logo-64.png</PackageIcon>
        <RepositoryUrl>https://github.com/Vooper-Media-LLC/Valour.TenorTwo</RepositoryUrl>
        <PackageTags>net7, tenor, api, valour</PackageTags>
    </PropertyGroup>
    <ItemGroup>
        <None Include="logo-64.png" Pack="true" PackagePath="" />
    </ItemGroup>
</Project>
